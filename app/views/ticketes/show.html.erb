<div id="tickete">
  <header>
    <h1><%= @project.name %></h1>
  </header>

  <header>
    <h2><%= @tickete.name %></h2>

    <ul class="actions">
      <li>
        <%= link_to "Edit Tickete", [:edit, @project, @tickete], class: "edit" %>
      </li>
      <li>
        <%= link_to "Delete Tickete",
        [@project, @tickete],
        method: :delete,
        data: { confirm: "are you sure you want to delete this tickete?" },
        class: "delete" %>
      </li>
    </ul>
  </header>

  <%= simple_format(@tickete.description) %>
</div>
