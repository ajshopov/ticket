<div id="tickete">
  <header>
    <h1><%= @project.name %></h1>
  </header>

  <header>
    <h2><%= @tickete.name %></h2>

    <ul class="actions">
      <% if policy(@tickete).update? %>
        <li>
          <%= link_to "Edit Tickete", [:edit, @project, @tickete], class: "edit" %>
        </li>
      <% end %>
      <% if policy(@tickete).destroy? %>
        <li>
          <%= link_to "Delete Tickete",
          [@project, @tickete],
          method: :delete,
          data: { confirm: "are you sure you want to delete this tickete?" },
          class: "delete" %>
        </li>
      <% end %>
    </ul>
  </header>

  <table id="attributes">
    <tr>
      <th>Author:</th>
      <td><%= @tickete.author.email %></td>
    </tr>
    <tr>
      <th>Created:</th>
      <td><%= time_ago_in_words(@tickete.created_at) %> ago</td>
    </tr>
  </table>

  <%= simple_format(@tickete.description) %>
</div>
